@article{McKinney2016,
  title = {An Integrated Linkage Map Reveals Candidate Genes Underlying Adaptive Variation in {{Chinook}} Salmon ( {{{\emph{Oncorhynchus}}}}{\emph{ Tshawytscha}} )},
