@techreport{Porter2019,
  title = {Movement and Distribution of {{Chinook Salmon}} Presmolts in the Mainstem {{Salmon River}}, 2018/2019 {{Annual Report}}},
  author = {Porter, Nicholas J. and Ackerman, Michael W. and Mackey, Tulley and Axel, Gordon A. and See, Kevin E.},
  year = {2019},
  institution = {{Prepared by Biomark, Inc. - Applied Biological Services and Fish Ecology Division, Northwest Fisheries Science Center, National Marine Fisheries Service, National Oceanic and Atmospheric Administration. 47 pp}},
  abstract = {Chinook salmon Oncorhynchus tshawytscha have two evolutionarily distinct life history types: ocean-type and stream-type. Within the stream-type lineage, there exists two juvenile migratory tactics; those leaving their natal tributaries as subyearlings prior to winter (presmolts) and, those emigrating from their natal areas 1 year after emergence (smolts). Within the upper Salmon River, Idaho, a research gap currently exists for presmolts that emigrate from the Lemhi River during fall and winter into the mainstem Salmon River, specifically in regard to overwinter movement, distribution, and habitat use. Using recently developed 0.5 g micro radio transmitters, 150 presmolts in the Lemhi River were implanted with tags, allowing for movement and distribution monitoring over the 2018-2019 winter in the mainstem Salmon River. Using a Cormack-Jolly-Seber model, we estimated that 2 radio-tagged presmolts transitioned (survived and moved with a live transmitter) through the Frank Church River of No Return Wilderness area of the Salmon River during the first 2 months with an overall transition probability of 1.4\%. Transition probabilities dropped 9\% upon entering a slack water area (Deadwater), 12\% upon entering a canyon section of the Salmon River, and 58\% reduction after entering the wilderness section of the Salmon River (likely due to battery failure or finding an area to overwinter). The mean travel speed was 5.6 km/d over 246 km. Further investigation will continue to assess movement and distribution of presmolts in the mainstem Salmon River over the next three years. This information will aid in addressing data gaps regarding use of mainstem winter rearing habitats by presmolts. Additionally, this project will assess habitat utilization of Chinook salmon smolts in natal tributaries.},
  file = {C\:\\Users\\mikea\\Zotero\\storage\\SVZBNB7X\\Porter2019.pdf}
}

@techreport{Porter2020,
  title = {Movement, {{Distribution}}, and {{Habitat Selection}} of {{Juvenile Chinook Salmon}} in the {{Upper Salmon Basin}}, 2019/2020 {{Annual Report}}},
  author = {Porter, Nicholas J. and See, Kevin E. and Ackerman, Michael W. and Hall, Mike and Carmichael, Richard A. and Axel, Gordon A.},
  year = {2020},
  institution = {{Biomark, Inc. - Applied Biological Services and Fish Ecology Division, Northwest Fisheries Science Center, National Marine Fisheries Service, National Oceanic and Atmospheric Administration}},
  abstract = {Chinook salmon Oncorhynchus tshawytscha have two evolutionarily distinct life history types: ocean-type and stream-type. Within the stream-type lineage, there exists two juvenile migratory tactics; those leaving their natal tributaries as subyearlings prior to winter (presmolts) and those emigrating from their natal areas 1 year after emergence (smolts). A gap in knowledge exists in the life history of juveniles regarding overwintering movement, distribution, and habitat use. Currently, we are in the third-year of a four-year study using new technology to determine overwinter survival and habitat selection of juvenile Chinook salmon. In October 2019, 282 pre-smolts in the Lemhi River at three collection locations upper Lemhi RST (LEMTRP), Hayden Creek RST (HYDTRP), and lower Lemhi RST (LLRTP) were implanted with 0.3 g radio transmitters to monitor movement and distribution over the 2019-2020 winter in the Salmon River. Using a Cormack-Jolly-Seber model in a Bayesian framework, we estimated detection probability at each fixed telemetry site, as well as the transition probability between each site (i.e. the probability that a fish will survive and move downstream). Cumulative transition probabilities dropped sharply upon entering the area just downstream of the LEMTRP tagging and again in a rehabilitation project area with quality habitat. The same areas also had some of the lowest estimated site to site transition probabilities with the addition of Salmon River Ranch to Colson Creek in the mainstem of the Salmon River. These lower transition probabilities may represent areas that juveniles occupied and therefore did not move downstream because of the quality habitat within these sites. We observed Chinook salmon presmolts throughout the lower Lemhi River using mobile telemetry a total of 66 times (habitat use events) with pools, terrestrial vegetation, and fine substrate being observed at the largest proportion of habitat use locations. These mobile surveys answered some of the questions regarding what these fish are doing between fixed sites while creating further questions of the fate of transmitters. Further investigation will continue to assess movement and distribution of presmolts in the mainstem Salmon River and Lemhi River in 2020-2021. This information will aid in addressing data gaps regarding use of mainstem winter rearing habitats by presmolts. Additionally, this project will continue to assess habitat availability and use of juvenile Chinook salmon in the lower Lemhi River to determine preferred habitat and inform target habitat conditions.},
  file = {C\:\\Users\\mikea\\Zotero\\storage\\PVGIV64X\\Porter2020.pdf}
}

