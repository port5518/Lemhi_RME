@article{Beacham2010,
  title = {A Comparison of Stock and Individual Identification for Sockeye Salmon ({{Oncorhynchus}} Nerka) in {{British Columbia}} Provided by Microsatellites and Single Nucleotide Polymorphisms},
  author = {Beacham, Terry D. and McIntosh, B. and Wallace, C.},
  editor = {Hansen, Michael},
  year = {2010},
