@article{Luikart2021,
  title = {Detecting Population Declines via Monitoring the Effective Number of Breeders ( {{{\emph{N}}}} {\textsubscript{b}} )},
  author = {Luikart, Gordon and Antao, Tiago and Hand, Brian K. and Muhlfeld, Clint C. and Boyer, Matthew C. and Cosart, Ted and Trethewey, Brian and Al-Chockhachy, Robert and Waples, Robin S.},
  year = {2021},
  month = feb,
  journal = {Molecular Ecology Resources},
  volume = {21},
  number = {2},
  pages = {379--393},
  issn = {1755-098X, 1755-0998},
  doi = {10.1111/1755-0998.13251},
  abstract = {Estimating the effective population size and effective number of breeders per year (Nb) can facilitate early detection of population declines. We used computer simulations to quantify bias and precision of the one-sample LDNe estimator of Nb in agestructured populations using a range of published species life history types, sample sizes, and DNA markers. Nb estimates were biased by \textasciitilde 5\%\textendash 10\% when using SNPs or microsatellites in species ranging from fishes to mosquitoes, frogs, and seaweed. The bias (high or low) was similar for different life history types within a species suggesting that life history variation in populations will not influence Nb estimation. Precision was higher for 100 SNPs (H {$\approx$} 0.30) than for 15 microsatellites (H {$\approx$} 0.70). Confidence intervals (CIs) were occasionally too narrow, and biased high when Nb was small (Nb {$<$} 50); however, the magnitude of bias would unlikely influence management decisions. The CIs (from LDNe) were sufficiently narrow to achieve high statistical power ({$\geq$}0.80) to reject the null hypothesis that Nb = 50 when the true Nb = 30 and when sampling 50 individuals and 200 SNPs. Similarly, CIs were sufficiently narrow to reject Nb = 500 when the true Nb = 400 and when sampling 200 individuals and 5,000 loci. Finally, we present a linear regression method that provides high power to detect a decline in Nb when sampling at least five consecutive cohorts. This study provides guidelines and tools to simulate and estimate Nb for age structured populations (https://github.com/ popgen gui/agestrucnb/), which should help biologists develop sensitive monitoring programmes for early detection of changes in Nb and population declines.},
  langid = {english},
  file = {C\:\\Users\\mikea\\Zotero\\storage\\R7G9L9AB\\Luikart et al. - 2021 - Detecting population declines via monitoring the e.pdf}
}

