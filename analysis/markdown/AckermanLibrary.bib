@article{Correa2017,
  title = {The Use of Genomic Information Increases the Accuracy of Breeding Value Predictions for Sea Louse ({{Caligus}} Rogercresseyi) Resistance in {{Atlantic}} Salmon ({{Salmo}} Salar)},
  author = {Correa, Katharina and Bangera, Rama and Figueroa, Ren{\'e} and Lhorente, Jean P. and Y{\'a}{\~n}ez, Jos{\'e} M.},
  year = {2017},
  month = dec,
  journal = {Genetics Selection Evolution},
  volume = {49},
  number = {1},
  pages = {15},
  issn = {1297-9686},
  doi = {10.1186/s12711-017-0291-8},
  abstract = {Sea lice infestations caused by Caligus rogercresseyi are a main concern to the salmon farming industry due to associated economic losses. Resistance to this parasite was shown to have low to moderate genetic variation and its genetic architecture was suggested to be polygenic. The aim of this study was to compare accuracies of breeding value predictions obtained with pedigree-based best linear unbiased prediction (P-BLUP) methodology against different genomic prediction approaches: genomic BLUP (G-BLUP), Bayesian Lasso, and Bayes C. To achieve this, 2404 individuals from 118 families were measured for C. rogercresseyi count after a challenge and genotyped using 37 K single nucleotide polymorphisms. Accuracies were assessed using fivefold cross-validation and SNP densities of 0.5, 1, 5, 10, 25 and 37 K. Accuracy of genomic predictions increased with increasing SNP density and was higher than pedigree-based BLUP predictions by up to 22\%. Both Bayesian and G-BLUP methods can predict breeding values with higher accuracies than pedigree-based BLUP, however, G-BLUP may be the preferred method because of reduced computation time and ease of implementation. A relatively low marker density (i.e. 10 K) is sufficient for maximal increase in accuracy when using G-BLUP or Bayesian methods for genomic prediction of C. rogercresseyi resistance in Atlantic salmon.},
  langid = {english},
  file = {C\:\\Users\\mikea\\Zotero\\storage\\YXTGFLDT\\Correa et al. - 2017 - The use of genomic information increases the accur.pdf}
}

@article{Courter2013,
  title = {Resident Rainbow Trout Produce Anadromous Offspring in a Large Interior Watershed},
  author = {Courter, Ian I. and Child, David B. and Hobbs, James A. and Garrison, Thomas M. and Glessner, Justin J.G. and Duery, Shadia},
  editor = {Fraser, Dylan},
